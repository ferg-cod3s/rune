# Rune Environment Variables
# Copy this file to .env and fill in your actual values

# Telemetry Configuration
# Segment Write Key - get this from your Segment workspace
# Used for analytics and user behavior tracking
RUNE_SEGMENT_WRITE_KEY=your_segment_write_key_here

# Sentry DSN - get this from your Sentry project  
# Used for error tracking, performance monitoring, and crash reporting
# Format: https://public_key@sentry.io/project_id
RUNE_SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id